#ifndef __ECU_CANBUSMESSAGES_H__
#define __ECU_CANBUSMESSAGES_H__

#include "Messages.hpp"
#include "PPhelp.h"

/**
 * @brief Defines the Can bus messages that will be received
 * 
 * Every definition is given a function that deals with the received message.
 * 
 * Unless it is the last entry, this is how to add a new message
 * 
 * X(CAN address, function) \
 * 
 * Only the last entry should NOT have a '\'
 * 
 * For now, all messages that should be used are in the namespace CanbusMsg
 */
#define CAN_MESSAGES             \
    X(0, CanbusMsg::BMSFaultMsg) \
    X(10, CanbusMsg::RMSFaultMsg)

#endif // __ECU_CANBUSMESSAGES_H__